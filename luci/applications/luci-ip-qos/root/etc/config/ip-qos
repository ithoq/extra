
config ip-qos 'main'
	option enable '0'
	option down '1280'
	option up '1280'

config ip-limit
	option ip '192.168.1.100'
	option enable '0'
	option upr '20'
	option upc '50'
	option downr '100'
	option downc '200'

config connlmt
	option enable '0'
	option ip '192.168.1.100'
	option tcp '200'
	option udp '100'

config port_first
	option enable '0'
	option proto 'tcp'
	option port '8080'

